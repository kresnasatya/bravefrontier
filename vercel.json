{
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/vercel/$1"
        },
        {
            "source": "/data/dbbs/raw.json",
            "destination": "/data/dbbs/raw.json"
        },
        {
            "source": "/data/omniunits/raw.json",
            "destination": "/data/omniunits/raw.json"
        }
    ],
    "functions": {
        "api/vercel/**/*.js": {
            "memory": 1024,
            "maxDuration": 10,
            "includeFiles": "data/**"
        }
    }
}